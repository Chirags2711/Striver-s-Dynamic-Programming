import java.util.Arrays;
import java.util.Scanner;
public class Solution {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		System.out.println(f(n));
	}
	public static int f(int n){
		//SPACE OPTIMIZATION
		int prev2=0, prev=1;
		for (int i = 2; i <= n; i++) {
			int curr = prev+prev2;
			prev2 = prev;
			prev = curr;
		}
		return prev;
	}
}
